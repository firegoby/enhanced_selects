<?xml version="1.0" encoding="UTF-8"?>
<extension id="enhanced_selects" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
    <name>Enhanced Selects</name>
	<description>Replace Admin select boxes with enhanced Select2 versions. Customizable blacklist and options per select.</description>
    <repo type="github">https://github.com/firegoby/enhanced_selects</repo>
	<!-- <url type="discuss">http://symphony-cms.com/discuss/thread/</url> -->
	<types>
		<type>Interface</type>
	</types>
	<authors>
		<author>
			<name github="firegoby" symphony="firegoby">Chris Batchelor</name>
			<email>chris@firegoby.com</email>
			<website>http://firegoby.com</website>
		</author>
	</authors>
	<releases>
		<release version="1.2.0" date="2016-03-17" min="2.3.x">
		- Add ignore support for Association UI Selector extension
		</release>
		<release version="1.1.0" date="2016-03-15" min="2.3.x">
		- Add ignore support for Field: Entry Relationship extension
		</release>
		<release version="1.0.1" date="2016-03-10" min="2.3.x">
		- Documentation and metadata fixes
		</release>
		<release version="1.0.0" date="2016-03-07" min="2.3.x">
		- Initial release
		</release>
	</releases>
    <media>
        <item type="image" url="https://raw.githubusercontent.com/firegoby/enhanced_selects/master/screenshots/pages.png">Enhanced Pages page</item>
        <item type="image" url="https://raw.githubusercontent.com/firegoby/enhanced_selects/master/screenshots/preferences.png">Enhanced Preferences page</item>
    </media>
</extension>
